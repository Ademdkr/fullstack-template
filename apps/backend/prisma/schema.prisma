generator client {
  provider   = "prisma-client-js"
  engineType = "library"   // Pflicht f√ºr Workers/Edge
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  directUrl  = env("DIRECT_DATABASE_URL")
}

model Budget {
  id        String   @id @default(cuid())
  name      String
  createdAt DateTime @default(now())
}
